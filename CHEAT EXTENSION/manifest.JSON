{
    "manifest_version": 2,
    "name": "Nyontek AI GOOGLE FORM",
    "version": "1.0",
    "description": "Penyontek GOOGLE FORM Handal",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}